{
  "name": "mobx-vue-lite",
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./nuxt": "./nuxt/module.mjs"
  },
  "files": [
    "dist",
    "nuxt"
  ],
  "sideEffects": true,
  "scripts": {
    "build-fast": "tsup src/index.ts --format esm,cjs",
    "build": "pnpm run build-fast -- --dts-resolve",
    "test": "vitest run",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "peerDependencies": {
    "mobx": "^6.1.0",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "mobx": "^6.4.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.1",
    "@types/node": "17.0.21",
    "@vue/test-utils": "2.0.1",
    "@vueuse/core": "8.7.3",
    "bumpp": "8.2.0",
    "eslint": "8.18.0",
    "happy-dom": "5.3.1",
    "tsup": "6.1.2",
    "typescript": "4.7.3",
    "vitest": "0.15.1"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
